class Solution(object):
    def maxProduct(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        maxFormer = nums[0]
        minFormer = nums[0]
        maxProduct = nums[0]
        for i in range (1,len(nums)):
            maxNew = max(max(maxFormer * nums[i], minFormer * nums[i]),nums[i])
            minNew = min(min(maxFormer * nums[i], minFormer * nums[i]),nums[i])
            maxProduct = max(maxNew, maxProduct)
            maxFormer = maxNew
            minFormer = minNew
        return maxProduct
