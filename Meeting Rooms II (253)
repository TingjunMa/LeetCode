def meeting(time):
    startTime = []
    endTime = []
    for meeting in time:
        startTime.append(meeting[0])
        endTime.append(meeting[1])
    startTime.sort()
    endTime.sort()
    i = j = 0
    numRooms = available = 0
    while i < len(startTime):
        if startTime[i] < endTime[j]:
            if available == 0:
                numRooms += 1
            else:
                available -= 1
            i += 1
        else:
            available += 1
            j += 1
    return numRooms
